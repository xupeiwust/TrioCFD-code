@article{streher_mixed_2021,
  title = {Mixed Modeling for Large-Eddy Simulation: {{The}} Single-Layer and Two-Layer Minimum-Dissipation-{{Bardina}} Models},
  shorttitle = {Mixed Modeling for Large-Eddy Simulation},
  author = {Streher, L. B. and Silvis, M. H. and Cifani, P. and Verstappen, R. W. C. P.},
  year = {2021},
  volume = {11},
  pages = {015002},
  publisher = {{American Institute of Physics}},
  abstract = {Predicting the behavior of turbulent flows using large-eddy simulation requires a modeling of the subgrid-scale stress tensor. This tensor can be approximated using mixed models, which combine the dissipative nature of functional models with the capability of structural models to approximate out-of-equilibrium effects. We propose a mathematical basis to mix (functional) eddy-viscosity models with the (structural) Bardina model. By taking an anisotropic minimum-dissipation (AMD) model for the eddy viscosity, we obtain the (single-layer) AMD\textendash Bardina model. In order to also obtain a physics-conforming model for wall-bounded flows, we further develop this mixed model into a two-layer approach: the near-wall region is parameterized with the AMD\textendash Bardina model, whereas the outer region is computed with the Bardina model. The single-layer and two-layer AMD\textendash Bardina models are tested in turbulent channel flows at various Reynolds numbers, and improved predictions are obtained when the mixed models are applied in comparison to the computations with the AMD and Bardina models alone. The results obtained with the two-layer AMD\textendash Bardina model are particularly remarkable: both first- and second-order statistics are extremely well predicted and even the inflection of the mean velocity in the channel center is captured. Hence, a very promising model is obtained for large-eddy simulations of wall-bounded turbulent flows at moderate and high Reynolds numbers.},
  file = {/home/martin/Zotero/storage/EKQYBJPS/Streher et al. - 2021 - Mixed modeling for large-eddy simulation The sing.pdf;/home/martin/Zotero/storage/TGAPIFV5/5.html},
  journal = {AIP Advances},
  number = {1}
}
