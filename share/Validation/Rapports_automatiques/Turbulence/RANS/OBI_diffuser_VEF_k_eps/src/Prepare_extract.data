
# Hydraulique 3 #

dimension 3

Domaine DOM_DOM

Read_unsupported_ASCII_file_from_ICEM DOM_DOM trio_DOM_geo.asc

Domaine REC_2D
Domaine REC_3D
Domaine IN_2D

Extract_2D_from_3D DOM_DOM in IN_2D 

dimension 2

Extruder_en3 { domaine 1 IN_2D nb_tranches 50 direction 0 0 -5 }

dimension 3

transformer IN_2D z-10.87 x y

Extract_2D_from_3D IN_2D left REC_2D

RegroupeBord REC_2D perio { devant derriere }

dimension 2

Pb_Hydraulique_Turbulent pb

VerifierCoin REC_2D { }

Ecrire_fichier REC_2D REC_2D.geom

VEFPreP1B dis

Schema_Euler_implicite sch
Read sch
{
	tinit 0.0
	tmax 150.
	dt_min 1.e-10
	dt_max 1.5
	dt_impr 1.0
	dt_sauv 1000.
	seuil_statio 1.e-8
	facsec_max 2000
	periode_sauvegarde_securite_en_heures 5
	solveur implicite
	{		
		seuil_convergence_solveur 1.e-12
		solveur gmres { diag seuil 1.e-12 impr controle_residu 1 }
	}
}


Associate pb REC_2D
Associate pb sch
Discretize pb dis

Read pb
{
   
   fluide_incompressible {
    	mu Champ_Uniforme 1 5.7e-5
   	rho Champ_Uniforme 1 1.
   }
   
           Navier_Stokes_turbulent
        {
          solveur_pression Cholesky { }
                convection { EF_stab { alpha 0.2 } }
                diffusion { }
                conditions_initiales
                        {
                                vitesse champ_uniforme 2 1. 0.
                        }
                boundary_conditions
                {
                            wall paroi_fixe
                            perio periodique 
                }
                Sources { Canal_perio { bord perio } }
                Modele_turbulence K_Epsilon
                {
                        Transport_K_Epsilon
                            {
                                convection { EF_stab { alpha 0.2 } }
                                diffusion { }
                                boundary_conditions
                                {
                                        wall Paroi
                                        perio periodique 
                                }
                                conditions_initiales { k_Eps champ_uniforme 2 1.e-6 1.e-9 }
                            }
                     turbulence_paroi loi_expert_hydr { methode_calcul_face_keps_impose que_les_faces_des_elts_dirichlet }
                 }
		Ecrire_fichier_xyz_valeur
		{
			dt 100.
			fields 2 k_eps vitesse
			boundaries 1 perio
		}		
		
		Traitement_particulier { Canal
			{
       			dt_impr_moy_spat 50.
       			}
					}

        }

        Postraitement
        {
       		Domaine REC_2D
		Sondes
                	{ 
                        sonde_vit  vitesse periode 0.1 segment 20 -14.0 3.7 -14.0 4.7
			sonde_visc_turb  viscosite_turbulente periode 0.1 segment 20 -14.0 3.7 -14.0 4.7 
			sonde_k K  periode 0.1 segment 20 -14.0 3.7 -14.0 4.7
			sonde_eps Eps  periode 0.1 segment 20 -14.0 3.7 -14.0 4.7 
			}


		Format lata
		Champs dt_post 100.
			{
			vitesse faces
			viscosite_turbulente elem
			pression elem
			y_plus elem
			k elem
			eps elem
			}
		
        }
}

Solve pb
End


 
