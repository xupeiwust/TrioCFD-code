# DISABLE_PARALLEL_RUN #
Parametres
{
  Auteur "G.B."
  
  Titre "Ascension gravitaire mono et multi-bulles periodique front-tracking en IJK"
  
  Description "Cette fiche verifie pas mal de choses dont : "
  Description "- La force de gravite"
  Description "- La periodicite"
  Description "- Les sauvegarde/reprise"
  Description "- La repulsion"

  CasTest . pre_run.data
  CasTest . ijkft_monte_seq.data
  CasTest . ijkft_monte_slow_seq.data
  CasTest . ijkft_monte_multi_seq.data
  CasTest . ijkft_monte_multi_repr_seq.data
  CasTest . ijkft_monte_par8.data 8
  CasTest . ijkft_monte_multi_par8.data 8
  CasTest . test_IJK_switch.data
  CasTest . test_IJK_switch_par8.data 8

  ExtraUsedFile init.lata.nodes init.lata.elem init_multi.lata.nodes init_multi.lata.elem
}

Chapitre {
	Titre "Maillage généré par cree_multi_bulles.sh"

	visu {
		titre "Etat initial genere par cree_multi_bulles.sh"
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh init.lata FTMESH
		pseudocolor init.lata FTMESH COMPO_CONNEXE ELEM
	}
	visu {
		titre "Etat initial genere par cree_multi_bulles.sh"
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh init_multi.lata FTMESH
		pseudocolor init_multi.lata FTMESH COMPO_CONNEXE ELEM
	}
}




Chapitre {
	Titre "Mono-bulle"
	Description "Calcul pre_run pour avancer la bulle au bout du domaine." <<EOF
Puis dans le calcul ijkft_monte_slow_seq il y a inversion de la bulle reelle et ghost.
La pression reagit au pas de temps suivant. 
EOF
	
	visu {
		titre "Fin du pre_run mono-bulle"
		cycles -1
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh        pre_run.lata DOM
		mesh        pre_run.lata INTERFACES
		pseudocolor pre_run.lata INTERFACES COMPO_CONNEXE ELEM
	}

	visu {
		titre "avant l'inversion"
		cycles 41
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh        ijkft_monte_slow_seq.lata DOM
		mesh        ijkft_monte_slow_seq.lata INTERFACES
		pseudocolor ijkft_monte_slow_seq.lata INTERFACES COMPO_CONNEXE ELEM
	}

	visu {
		titre "inversion"
		cycles 42
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh        ijkft_monte_slow_seq.lata DOM
		mesh        ijkft_monte_slow_seq.lata INTERFACES
		pseudocolor ijkft_monte_slow_seq.lata INTERFACES COMPO_CONNEXE ELEM
	}

	visu {
		titre "apres l'inversion"
		cycles 43
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh        ijkft_monte_slow_seq.lata DOM
		mesh        ijkft_monte_slow_seq.lata INTERFACES
		pseudocolor ijkft_monte_slow_seq.lata INTERFACES COMPO_CONNEXE ELEM
	}


	visu {
		titre "avant l'inversion"
		cycles 41
		pseudocolor ijkft_monte_slow_seq.lata DOM PRESSURE ELEM
		Operator Slice2D_z 0. 0. 1.5e-3
	}

	visu {
		titre "inversion"
		cycles 42
		pseudocolor ijkft_monte_slow_seq.lata DOM PRESSURE ELEM
		Operator Slice2D_z 0. 0. 1.5e-3
	}

	visu {
		titre "apres l'inversion"
		cycles 43
		pseudocolor ijkft_monte_slow_seq.lata DOM PRESSURE ELEM
		Operator Slice2D_z 0. 0. 1.5e-3
	}


	Description "La vitesse de montee de bulle est bien continue au changement de cote." <<EOF
Apres la mise en regime du pre_run, la vitesse de montee est constante. 
EOF

	figure {
		titre "Position des bulles en x"	  
		incluredesccourbes 0
		labelX  "t"
		labelY  "x"
		Courbe {
		   fichier pre_run_bulles_centre_x.out
		     colonnes ($1) ($2)
		     Legende "pre_run col2 ori"
		}
		Courbe {
		   fichier pre_run_bulles_centre_x.out
		     colonnes ($1) (($2)+0.006)
		     Legende "pre_run col2 translate"
		     }
		Courbe {
		   fichier ijkft_monte_slow_seq_bulles_centre_x.out
		     colonnes ($1) ($2)
		     Legende "ijkft_monte_slow_seq col2 ori"
		}
		Courbe {
		   fichier ijkft_monte_slow_seq_bulles_centre_x.out
		     colonnes ($1) (($2)+0.006)
		     Legende "ijkft_monte_slow_seq col2 translate"
		     }
		Courbe {
		   fichier ijkft_monte_seq_bulles_centre_x.out
		     colonnes ($1) ($2)
		     Legende "ijkft_monte_seq col2 ori"
		}
		Courbe {
		   fichier ijkft_monte_seq_bulles_centre_x.out
		     colonnes ($1) (($2)+0.006)
		     Legende "ijkft_monte_seq col2 translate"
		     }
	}

}


