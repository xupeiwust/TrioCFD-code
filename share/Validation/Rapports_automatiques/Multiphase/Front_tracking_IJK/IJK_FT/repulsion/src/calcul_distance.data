# PARALLEL ONLY 8 #
dimension 3
Probleme_FTD_IJK pb

Domaine_IJK DOM
Lire DOM
{
  nbelem 32 32 32
  size_dom 0.008 0.008 0.008
  perio 1 1 0
  nproc 2 2 2
  ijk_splitting_ft_extension 24
}

# RK3_IJK sch #
schema_euler_explicite_IJK sch

lire sch {
  tinit 0.0
  timestep 0.001
  timestep_facsec 0.4
  nb_pas_dt_max 1 
  dt_sauvegarde 1000
  cfl 1.0
  fo  1.0
  oh  1.0
}

option_ijk { check_divergence }
IJK dis

associer sch pb
associer DOM pb
Discretize pb dis

lire pb
{
  SOLVED_EQUATIONS
  {
    Navier_Stokes_FTD_IJK hydraulique
    IJK_Interfaces interf
  }
  
  Fluide_Diphasique_IJK
  {
    # Give a number for each phase #
    fluide0 Fluide_Incompressible
    {
        mu  Champ_Uniforme 1 1.e-3
        rho Champ_Uniforme 1 800.   # rho_vapeur # 
    }
    fluide1 Fluide_Incompressible # Liquid # 
    {
        mu  Champ_Uniforme 1 1.e-3
        rho Champ_Uniforme 1 1000.
    }
    # Surface tension #
    sigma   Champ_Uniforme 1 0.07

    # Gravity field #
    gravite Champ_Uniforme 3 0. 0. -9.81
  }

  hydraulique
  {
        Boundary_Conditions 
        {
          bctype_kmin paroi
          bctype_kmax paroi
        }

  multigrid_solver {
    solver_precision mixed
    coarsen_operators 3
      Coarsen_Operator_Uniform {  }
      Coarsen_Operator_Uniform {  }
      Coarsen_Operator_Uniform {  } 	
    ghost_size 1
    pre_smooth_steps 1 7
    smooth_steps 1 7
    relax_jacobi 1 0.7
    solveur_grossier GCP { seuil 1e-11  precond ssor { omega 1.5 } }
    seuil 1e-10
    nb_full_mg_steps 2 4 1
  }
  expression_vx_init 1.0
  expression_vy_init 0.8
  expression_vz_init 0.0  
  }
  
  interf 
  {
    fichier_reprise_interface init2.lata
    timestep_reprise_interface       1
    lata_meshname   FTMESH
    compute_distance_autres_interfaces
    portee_force_repulsion 0.003
    delta_p_max_repulsion 1
       remaillage_ft_ijk {
        nb_iter_barycentrage 1
        relax_barycentrage 1
        nb_iter_correction_volume 2
        lissage_courbure_iterations_systematique 0
    }
 
  }
  
 {
 
 }
 
  post_processing 
  {
    format lata
    nb_pas_dt_post 1
    fields 
    {
       Velocity faces
    }
interfaces interf
    {
       courbure som
	DISTANCE_AUTRES_INTERFACES SOM
    }
  }
}
solve pb
# Lata_to_other lml NOM_DU_CAS NOM_DU_CAS #
Fin
