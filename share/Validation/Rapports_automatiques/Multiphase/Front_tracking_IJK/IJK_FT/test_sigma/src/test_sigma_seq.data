IJK_Grid_Geometry DOM

Lire DOM
{
  nbelem_i 64
  nbelem_j 64
  nbelem_k 32
  uniform_domain_size_i 0.64
  uniform_domain_size_j 0.64
  uniform_domain_size_k 0.32
  perio_i
  perio_j
}

IJK_Splitting grid_splitting
Lire grid_splitting
{
  ijk_grid_geometry DOM
  nproc_i 1
  nproc_j 1
  nproc_k 1
}

IJK_FT_double
{
  Boundary_Conditions {
    bctype_kmin Symetrie
    bctype_kmax Symetrie
  }
  ijk_splitting grid_splitting
  ijk_splitting_ft_extension 22
  timestep 0.005
  timestep_facsec 0.4
  dt_post 10
  # champs_a_postraiter 1 tous #
  champs_a_postraiter 10 INTERFACES COURBURE COMPO_CONNEXE VELOCITY PRESSURE REPULSION_FT SOURCE_QDM_INTERF CELL_RHO_SOURCE_QDM_INTERF RHO CELL_SOURCE_QDM_INTERF
  check_stop_file stop.txt
  dt_sauvegarde 1000
  nb_pas_dt_max 1280

  multigrid_solver {
    solver_precision double
    coarsen_operators 3
    Coarsen_Operator_Uniform { }
    Coarsen_Operator_Uniform { }
    Coarsen_Operator_Uniform { }
    ghost_size 1
    pre_smooth_steps 1 7
    smooth_steps 1 7
    relax_jacobi 1 0.7
    solveur_grossier GCP { seuil 1e-11  precond ssor { omega 1.5 } }
    seuil 1e-10
    nb_full_mg_steps 2 20 1
    impr
  }
  # LECTURE DES INTERFACES DANS UN FICHIER LATA #
  interfaces
  {
    # Soit un fichier genere externe #
    # Choix1 #
    fichier_reprise_interface prepare.sauv.lata
    timestep_reprise_interface 1
    lata_meshname INTERFACES
  }
  # FIN AJOUT #
  check_divergence
  gravite 3 0. 0. 0.
  # vitesse_entree 0.01 #
  rho_liquide 1000.
  mu_liquide 1e-3
  sigma 1.
  expression_vx_init 1.0*0.1
  expression_vy_init 1.2*0.1
  expression_vz_init 0.0
}
Fin
