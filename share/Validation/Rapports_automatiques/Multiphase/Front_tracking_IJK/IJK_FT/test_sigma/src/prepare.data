IJK_Grid_Geometry DOM

Lire DOM
{
  nbelem_i 64
  nbelem_j 40
  nbelem_k 32
  uniform_domain_size_i 0.64
  uniform_domain_size_j 0.40
  uniform_domain_size_k 0.32
  perio_i
  perio_j
}

IJK_Splitting grid_splitting
Lire grid_splitting
{
  ijk_grid_geometry DOM
  nproc_i 1
  nproc_j 1
  nproc_k 1
}

IJK_FT_double
{
  Boundary_Conditions {
    bctype_kmin Symetrie
    bctype_kmax Symetrie
  }
  ijk_splitting grid_splitting
  ijk_splitting_ft_extension 0
  timestep 0.000001
  timestep_facsec 0.4
  dt_post 1
  champs_a_postraiter 1 tous
  check_stop_file stop.txt
  dt_sauvegarde 1000
  nb_pas_dt_max 5

  multigrid_solver {
    solver_precision mixed
    coarsen_operators 3
    Coarsen_Operator_Uniform { }
    Coarsen_Operator_Uniform { }
    Coarsen_Operator_Uniform { }
    ghost_size 1
    pre_smooth_steps 1 7
    smooth_steps 1 7
    relax_jacobi 1 0.7
    solveur_grossier GCP { seuil 1e-8  precond ssor { omega 1.5 } }
    seuil 1e-6
    nb_full_mg_steps 2 4 1

  }
  # LECTURE DES INTERFACES DANS UN FICHIER LATA #
  interfaces
  {
    # Soit un fichier genere externe #
    # Choix1 #
    fichier_reprise_interface init.lata
    timestep_reprise_interface       1
    lata_meshname   FTMESH
    # Bary et lissage booste #
    remaillage_ft_ijk {
	    nb_iter_barycentrage 100
	    relax_barycentrage 1
	    nb_iter_correction_volume 10
	    lissage_courbure_iterations_systematique 100
    }
  }
  # FIN AJOUT #
  check_divergence
  gravite 3 0. 0. 0.
  # vitesse_entree 0.01 #
  rho_liquide 1000.
  mu_liquide 1e-3
  sigma 1.
  expression_vx_init 0.
  expression_vy_init 0.
  expression_vz_init 0.
}
Fin
