Parametres {
	Titre "Ascension gravitaire mono et multi-bulles periodique front-tracking en IJK"
	Description "Cette fiche verifie pas mal de choses dont : " <<EOF
o La force de gravite
o La periodicite 
o Les sauvegarde/reprise
o La repulsion
		In order to validate the algorithm implemented for the resolution of the energy equation, we performed some particular test cases in which numerical solutions are compared to the corresponding analytical ones.
	\latex_($$\rho c_p \left( \partial_t T + \mathbf{u}.\nabla T \right) = \nabla (\lambda \nabla T)$$\latex_)
	To this equation, we add spatial boundary conditions (BC) and an initial condition.
	\latex_(\begin{center} \begin{tabular}{|c|c|c|c|c|c|c|} \hline $L_x$ & $L_y$ & $L_z$ & $N_x$ & $N_y$ & $N_z$ & $\Delta t$ \\ \hline 0.006 &0.006 & 0.003 & 64 & 64 & 32 & 2.4e-4 \\  \hline \end{tabular} \end{center}\latex_)
	Where L, N and $\Delta t$ stand respectively for the length, number of nodes and time step. The subscripts $x$, $y$ and $z$ indicate the different directions.
EOF
	Auteur "G.B."
	CasTest . test_CL_RK3.data
	CasTest . pre_run.data
	CasTest . ijkft_basic_temperature_seq.data
	CasTest . ijkft_basic_temperature_seq_repr.data
 	CasTest . ijkft_monte_slow_seq.data
 	CasTest . ijkft_monte_multi_seq.data
 	CasTest . ijkft_monte_multi_repr_seq.data
 	CasTest . ijkft_monte_par8.data 8
 	CasTest . ijkft_monte_multi_par8.data 8
 	# DISABLE_PARALLEL_RUN #
}
Chapitre {
	Titre "Mesh generated by cree_multi_bulles.sh"

	visu {
		titre "Etat initial genere par cree_multi_bulles.sh"
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh init.lata FTMESH
		pseudocolor init.lata FTMESH COMPO_CONNEXE ELEM
	}
	visu {
		titre "Etat initial genere par cree_multi_bulles.sh"
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh init_multi.lata FTMESH
		pseudocolor init_multi.lata FTMESH COMPO_CONNEXE ELEM
	}
}

Chapitre {
	Titre "Evolution de la temperature avec des conditions aux limites de temperature imposee a la paroi"

	visu {
		titre "Calculated solution"
		cycles -1
		width 12cm
		pseudocolor ijkft_basic_temperature_seq.lata DOM TEMPERATURE_0 ELEM
		mesh INTERFACES
	}

}



Chapitre {
	Titre "Mono-bulle"
	Description "Calcul pre_run pour avancer la bulle au bout du domaine." <<EOF
Puis dans le calcul ijkft_monte_slow_seq il y a inversion de la bulle reelle et ghost.
La pression reagit au pas de temps suivant. 
EOF
	
	visu {
		titre "Fin du pre_run mono-bulle"
		cycles -1
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh        pre_run.lata DOM
		mesh        pre_run.lata INTERFACES
		pseudocolor pre_run.lata INTERFACES COMPO_CONNEXE ELEM
	}

	visu {
		titre "avant l'inversion"
		cycles 41
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh        ijkft_monte_slow_seq.lata DOM
		mesh        ijkft_monte_slow_seq.lata INTERFACES
		pseudocolor ijkft_monte_slow_seq.lata INTERFACES COMPO_CONNEXE ELEM
	}

	visu {
		titre "inversion"
		cycles 42
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh        ijkft_monte_slow_seq.lata DOM
		mesh        ijkft_monte_slow_seq.lata INTERFACES
		pseudocolor ijkft_monte_slow_seq.lata INTERFACES COMPO_CONNEXE ELEM
	}

	visu {
		titre "apres l'inversion"
		cycles 43
		Normal3d 0 0 1
		Up3D 0 1 0
		zoom3D 0 0 1.
		mesh        ijkft_monte_slow_seq.lata DOM
		mesh        ijkft_monte_slow_seq.lata INTERFACES
		pseudocolor ijkft_monte_slow_seq.lata INTERFACES COMPO_CONNEXE ELEM
	}


	visu {
		titre "avant l'inversion"
		cycles 41
		pseudocolor ijkft_monte_slow_seq.lata DOM PRESSURE ELEM
		Operator Slice2D_z 0. 0. 1.5e-3
	}

	visu {
		titre "inversion"
		cycles 42
		pseudocolor ijkft_monte_slow_seq.lata DOM PRESSURE ELEM
		Operator Slice2D_z 0. 0. 1.5e-3
	}

	visu {
		titre "apres l'inversion"
		cycles 43
		pseudocolor ijkft_monte_slow_seq.lata DOM PRESSURE ELEM
		Operator Slice2D_z 0. 0. 1.5e-3
	}


	Description "La vitesse de montee de bulle est bien continue au changement de cote." <<EOF
Apres la mise en regime du pre_run, la vitesse de montee est constante. 
EOF

	figure {
		titre "Position des bulles en x"	  
		incluredesccourbes 0
		labelX  "t"
		labelY  "x"
		Courbe {
		   fichier pre_run_bulles_centre_x.out
		     colonnes ($1) ($2)
		     Legende "pre_run col2 ori"
		}
		Courbe {
		   fichier pre_run_bulles_centre_x.out
		     colonnes ($1) (($2)+0.006)
		     Legende "pre_run col2 translate"
		     }
		Courbe {
		   fichier ijkft_monte_slow_seq_bulles_centre_x.out
		     colonnes ($1) ($2)
		     Legende "ijkft_monte_slow_seq col2 ori"
		}
		Courbe {
		   fichier ijkft_monte_slow_seq_bulles_centre_x.out
		     colonnes ($1) (($2)+0.006)
		     Legende "ijkft_monte_slow_seq col2 translate"
		     }
		Courbe {
		   fichier ijkft_monte_seq_bulles_centre_x.out
		     colonnes ($1) ($2)
		     Legende "ijkft_monte_seq col2 ori"
		}
		Courbe {
		   fichier ijkft_monte_seq_bulles_centre_x.out
		     colonnes ($1) (($2)+0.006)
		     Legende "ijkft_monte_seq col2 translate"
		     }
	}

}


