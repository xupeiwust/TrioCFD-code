# PARALLEL ONLY 4 #
# Le decoupage ne fait rien #
# Astuce pour que make_PAR.data trouve deux processeurs de calcul:
Nom_Zones toto
#
# DEBUT DECOUPAGE
system "touch toto_0000.Zones toto_0001.Zones toto_0002.Zones toto_0003.Zones"
Fin
  FIN DECOUPAGE #
dimension 3
Probleme_FTD_IJK pb

Domaine_IJK DOM
Lire DOM
{
  nbelem 128 32 32
  size_dom 0.02 0.005 0.005
  perio 1 1 1
  # DEBUT MAILLAGE #
  nproc 1 1 1
  # FIN MAILLAGE #
  # DEBUT LECTURE #
  nproc 1 1 4
  # FIN LECTURE #
  ijk_splitting_ft_extension 10 # To be checked.. #
}

Schema_RK3_IJK sch
lire sch
{
  tinit 0.0
	nb_pas_dt_max 3
  timestep 0.9e-4
  timestep_facsec 1.0
  dt_sauvegarde 50
  cfl 1.0
  fo  1.0
  oh  1.0
}

IJK dis

associer sch pb
associer DOM pb
Discretize pb dis

Option_IJK
{
  check_divergence
  # disable_diphasique #
}

lire pb
{
  SOLVED_EQUATIONS
  {
    Navier_Stokes_FTD_IJK hydraulique
    ijk_interfaces interf
  }

  Fluide_Diphasique_IJK
  {
    # Give a number for each phase #
    # vapeur #
    fluide0 Fluide_Incompressible
    {
        mu  Champ_Uniforme 1 1.3493e-05
        rho Champ_Uniforme 1 87.545
    }
    # liquide #
    fluide1 Fluide_Incompressible
    {
        mu  Champ_Uniforme 1 0.001578
        rho Champ_Uniforme 1 1171.3
    }
    # Surface tension #
    sigma   Champ_Uniforme 1 0.018055

    # Gravity field #
    gravite Champ_Uniforme 3 -9.81 0. 0.
  }


  hydraulique
  {
    velocity_convection_op Centre
    vol_bulle_monodisperse 5.23583e-10
    # ajout des mots clefs suivants pour fixer les bulles #
    coef_mean_force 0.0
    coef_force_time_n 0.0
    coef_immobilisation 18810000 # doit etre de l ordre de grandeur rho*g/Dx. Dx est la taille d une maille #
    coef_ammortissement -3000 # -cv = -kDx >> c = k Dx / v. v est la vitesse max qu'on autorise a la bulle #
    coef_rayon_force_rappel 1.5 # a garder a l esprit pour les bulles deformables #
    # timestep_reprise_vitesse 1 #
    Boundary_Conditions
    {
      bctype_kmin perio
      bctype_kmax perio
    }

    multigrid_solver
    {
      solver_precision mixed
      coarsen_operators 0
      ghost_size 1
      pre_smooth_steps 1 7
      smooth_steps 1 7
      relax_jacobi 1 0.7
      solveur_grossier GCP { seuil 1e-10  precond ssor { omega 1.5 }  }
      seuil 1e-8
      nb_full_mg_steps 2 20 1
    }
    terme_force_init 10855.866965787514 # = (0.059688461999999984*87.545+(1.0-0.059688461999999984)*1171.3)*9.81 #
    expression_derivee_force 20000*rappel_moyen
    # refuse_patch_conservation_QdM_RK3_source_interf #
    expression_vx_init 0.
    expression_vy_init 0.
    expression_vz_init 0.
	}

  # LECTURE DES INTERFACES DANS UN FICHIER LATA #
  interf
  {
    terme_gravite rho_g
    fichier_reprise_interface init.lata
    timestep_reprise_interface       1
    lata_meshname   FTMESH
    remaillage_ft_ijk
    {
      pas_remaillage 1e-6
      nb_iter_barycentrage 1
      relax_barycentrage 1.00000000000000000e+00
      critere_arete 3.49999999999999978e-01
      seuil_dvolume_residuel 0.00000000000000000e+00
      nb_iter_correction_volume 6
      nb_iter_remaillage 6
      facteur_longueur_ideale 7.50000000000000000e-01
      lissage_courbure_coeff -2.50000000000000014e-02
      lissage_courbure_iterations_systematique 1
      lissage_courbure_iterations_si_remaillage 10
    }
  # follow_colors #
  }
  {
    # Pour Creer le fichier de Reprise #
    nom_sauvegarde diph_fixe.sauv
    # REPRISE #
    # nom_reprise diph_init.sauv #
    sauvegarder_xyz
    # champs_a_postraiter 11 COMPO_CONNEXE INDICATRICE VELOCITY PRESSURE COURBURE INTERFACES EXTERNAL_FORCE INTEGRATED_VELOCITY INTEGRATED_PRESSURE INTEGRATED_TIMESCALE INDICATRICE_PERTURBE #
  }
  post_processing 
  {
    format LATA
    nb_pas_dt_post 50
    nb_pas_dt_post_stats_bulles 5
    nb_pas_dt_post_stats_plans 50
    t_debut_statistiques 0.0
    
    # reset_reprise_integrated #
    fields 
    {
       indicatrice elem
       num_compo elem
       PRESSURE ELEM
       VELOCITY ELEM
       EXTERNAL_FORCE ELEM
       INTEGRATED_VELOCITY ELEM
       INTEGRATED_PRESSURE ELEM
       INTEGRATED_TIMESCALE ELEM
       INDICATRICE_PERTURBE ELEM
    }

    interfaces interf
    {
       courbure som
    } 
  }
}
solve pb
Fin
