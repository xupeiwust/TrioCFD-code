# PARALLEL OK #
Dimension 3
Pb_Thermohydraulique_Turbulent pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
	Pave Cavite
	{
		Origine 0 0.047619 0
		Nombre_de_Noeuds 12 8 7
		Longueurs 6.4 1.904762 3.2
		Facteurs 1 1 1
	}
	{
		Bord PerioX	X = 0		0.047619 <= Y <= 1.952381	0 <= Z <= 3.2
		Bord PerioX	X = 6.4		0.047619 <= Y <= 1.952381	0 <= Z <= 3.2
		Bord PerioZ	Z = 0		0 <= X <= 6.4			0.047619 <= Y <= 1.952381
		Bord PerioZ	Z = 3.2		0 <= X <= 6.4			0.047619 <= Y <= 1.952381
		Bord Bas	Y = 0.047619	0 <= X <= 6.4			0 <= Z <= 3.2
		Bord Haut	Y = 1.952381	0 <= X <= 6.4			0 <= Z <= 3.2
	}
}

Tetraedriser_homogene_fin dom
Corriger_frontiere_periodique { domaine dom bord PerioX }
Corriger_frontiere_periodique { domaine dom bord PerioZ }
# END MESH #

# BEGIN PARTITION
Partition dom
{
        Partitionneur metis { Nb_parts 3 }
        Larg_joint 2
	periodique 2 PerioX PerioZ
        Nom_Zones DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1b dis

Runge_Kutta_ordre_3 sch_RK3
Read sch_RK3
{
	tinit 0
	nb_pas_dt_max 3
	tmax 150
	dt_start dt_calc
	dt_min 1e-7
	dt_max 1
	dt_impr 1
	dt_sauv 100
	seuil_statio 1e-15
	facsec 1
	no_check_disk_space
	periode_sauvegarde_securite_en_heures 11
}



Associate pb dom
Associate pb sch_RK3

Discretize pb dis

Read pb
{
   
   fluide_incompressible {
   
        gravite champ_uniforme 3 0 0 0
   	mu	champ_uniforme 1 2.84e-5
    	rho	champ_uniforme 1 0.011928
   	lambda	champ_uniforme 1 0.20772
   	Cp	champ_uniforme 1 5193
   	beta_th	champ_uniforme 1 1.9954e-3
   }
   
   	Navier_Stokes_turbulent
	{
		Solveur_pression	petsc Cholesky { }
		Convection		{ EF_stab { volumes_etendus alpha 0.2 } }
		Diffusion		{ }
		Conditions_initiales	{ vitesse champ_init_canal_sinal 3 { Ucent 24.293 h 1 ampli_sin 0 omega 1 ampli_bruit 0.0 } }
		boundary_conditions	{
					PerioX	periodique
					PerioZ	periodique
					Haut	paroi_decalee_Robin { delta 0.047619 }
					Bas	paroi_decalee_Robin { delta 0.047619 }
					}
		Modele_turbulence sous_maille_WALE
					{
					turbulence_paroi loi_standard_hydr
					dt_impr_ustar 5
					}
		Traitement_particulier	{
					canal	{
						dt_impr_moy_spat 50
						dt_impr_moy_temp 50
						debut_stat 100
						}
					}
		Sources			{ 
		                          source_Robin 2 Haut Bas # 0.001 # ,
		                          canal_perio { bord PerioX } 
		                        }
	}

	Convection_diffusion_temperature_turbulent
	{
		Convection		{ EF_stab { volumes_etendus alpha 1 } }
		Diffusion		{ }
		Conditions_initiales	{ temperature champ_fonc_xyz dom 1 0 }
		boundary_conditions	{
					PerioX	periodique
					PerioZ	periodique
					Haut	paroi_decalee_Robin { delta 0.047619 }
					Bas	paroi_decalee_Robin { delta 0.047619 }
					}
		Modele_turbulence Prandtl
					{
					turbulence_paroi loi_standard_hydr_scalaire
					dt_impr_nusselt 5
					}
		Sources			{
		                          source_Robin_scalaire 2 Haut 0 Bas 0 # 0.001 # ,
		                          puissance_thermique champ_uniforme 1 1
		                        }
	}

	Postraitement
	{
	Definition_champs		{
					moyenne_vitesse		Moyenne { t_deb 100 t_fin 150 source refChamp { Pb_champ pb vitesse } }
					moyenne_temperature	Moyenne { t_deb 100 t_fin 150 source refChamp { Pb_champ pb temperature } }
					ecart_type_vitesse	Ecart_type { t_deb 100 t_fin 150 source refChamp { Pb_champ pb vitesse } }
					ecart_type_temperature	Ecart_type { t_deb 100 t_fin 150 source refChamp { Pb_champ pb temperature } }
					min_temperature		Reduction_0D { methode min source refChamp { Pb_champ pb temperature } }
					max_temperature		Reduction_0D { methode max source refChamp { Pb_champ pb temperature } }
					}
		Sondes			{
					sonde_vitesse			nodes	vitesse			periode 0.1 points 1 3.2 1 1.6
					sonde_temperature		nodes	temperature		periode 0.1 points 1 3.2 1 1.6
					sonde_moyenne_vitesse		nodes	moyenne_vitesse		periode 0.1 points 1 3.2 1 1.6
					sonde_moyenne_temperature	nodes	moyenne_temperature	periode 0.1 points 1 3.2 1 1.6
					sonde_ecart_type_vitesse	nodes	ecart_type_vitesse	periode 0.1 points 1 3.2 1 1.6
					sonde_ecart_type_temperature	nodes	ecart_type_temperature	periode 0.1 points 1 3.2 1 1.6

					coupe_vitesse			nodes	vitesse			periode 1 segment 15 0.096970 0.047619 0.088889	0.096970 1.952381 0.088889
					coupe_temperature		nodes	temperature		periode 1 segment 15 0.096970 0.047619 0.088889	0.096970 1.952381 0.088889
					coupe_moyenne_vitesse		nodes	moyenne_vitesse		periode 1 segment 15 0.096970 0.047619 0.088889	0.096970 1.952381 0.088889
					coupe_moyenne_temperature	nodes	moyenne_temperature	periode 1 segment 15 0.096970 0.047619 0.088889	0.096970 1.952381 0.088889
					coupe_ecart_type_vitesse	nodes	ecart_type_vitesse	periode 1 segment 15 0.096970 0.047619 0.088889	0.096970 1.952381 0.088889
					coupe_ecart_type_temperature	nodes	ecart_type_temperature	periode 1 segment 15 0.096970 0.047619 0.088889	0.096970 1.952381 0.088889
					}
		Format lml
		Champs dt_post 50	{
					vitesse som
					temperature som
					min_temperature som
					max_temperature som
					}
		Statistiques dt_post 50
					{
					t_deb 100 t_fin 150
					moyenne vitesse
					moyenne temperature
					ecart_type vitesse
					ecart_type temperature
					}
	}
	sauvegarde formatte pb.sauv
}

Solve pb
End
 
